{
  "name": "tilly-vscode-extension",
  "version": "0.1.0",
  "description": "Tilly static analysis tool frontend",
  "main": "./out/extension",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -b",
    "watch": "tsc -b -w"
  },
  "engines": {
    "vscode": "^1.43.0"
  },
  "activationEvents": [
    "onLanguage:plaintext"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sunziping2016/tilly-vscode-extension.git"
  },
  "author": "Sun Ziping",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sunziping2016/tilly-vscode-extension/issues"
  },
  "homepage": "https://github.com/sunziping2016/tilly-vscode-extension#readme",
  "dependencies": {
    "vscode-jsonrpc": "^5.0.1",
    "vscode-languageclient": "^6.1.3"
  },
  "devDependencies": {
    "@types/node": "^12.12.0",
    "@types/vscode": "1.43.0",
    "eslint": "^6.4.0",
    "typescript": "^4.0.2"
  }
}
